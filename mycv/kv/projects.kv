<Projects>:
    ScrollView:
        GridLayout:
            #rows: 3
            cols: 1
            size_hint_y: None
            height: self.minimum_height
            row_default_height: "200dp"
            row_force_default: True
            padding: 30
            spacing: 30

            # spacial
            ImageButton:
                source: "mycv/img/spacial.jpg"
                on_release: app.alternar ("spacial")

