            ImageButton:
                source: "mycv/img/telas.jpg"
                on_release: app.alternar ("colony")

            # site
            ImageButton:
                source: "mycv/img/site.jpg"
                on_release: app.alternar ("site")

            # maze
            ImageButton:
                source: "mycv/img/labirinto 1.jpg"
                on_release: app.alternar ("maze")

            # outback
            ImageButton:
                source: "mycv/img/sertao 5.jpg"
                on_release: app.alternar ("outback")

            # fight
            ImageButton:
                source: "mycv/img/luta 3.jpg"
                on_release: app.alternar ("fight")

            # platform
            ImageButton:
                source: "mycv/img/fic 2.jpg"
