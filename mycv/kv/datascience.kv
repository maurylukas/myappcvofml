<Data_Science>:
    ScrollView:
        GridLayout:
            #rows: 3
            cols: 1
            size_hint_y: None
            height: self.minimum_height
            row_default_height: "200dp"
            #row_force_default: True
            padding: 20
            spacing: 20

            LabelButton:
                text: "Exploratory Data Analysis and Machine Learning Algorithmic Models to help people predict Airbnb rental prices in Lisbon, Portugal. Click here to go to the Notebook on Kaggle website."
                italic: True
                text_size: self.width, None
                #size_hint: 1, None
                height: self.texture_size [1]
                on_release: webbrowser.open ("https://www.kaggle.com/maurylukas/listings/code")
            Image:
                source: "mycv/img/kag.jpg"
